Template ProPlan version 1.0			
	Provider "Papamoscas SL" as Responder;

	Metrics:
		int: integer [1..10000];
		price: integer [0..500];

	AgreementTerms
		Service BirdAPI availableAt "http://papamoscas-isa.appspot.com/api/v4/birds"
			Features: ;
			GlobalDescription
				planPrice: price = 999;

		MonitorableProperties
			global:
				avgResponseTime: int;

		GuaranteeTerms
			TimeTerm: Provider guarantees avgResponseTime <= 500;
EndTemplate