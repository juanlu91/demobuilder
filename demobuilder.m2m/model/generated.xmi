<?xml version="1.0" encoding="ISO-8859-1"?>
<mmag:AgreementModel xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mmag="http://agreement.metamodel/1.0" id="ProPlan" version="1.0">
  <context>
    <actors id="Amazon"/>
    <metrics id="int">
      <domain xsi:type="mmag:Range" min="-9999999.0" max="9999999.0"/>
    </metrics>
  </context>
  <agTerm>
    <serviceConfigurarion endpointReference="s3.amazon.com" serviceName="S3">
      <features>
        <parameters/>
      </features>
      <configurationProperties/>
    </serviceConfigurarion>
    <monitorableProperties features="//@agTerm/@serviceConfigurarion/@features.0"/>
    <guaranteeTerms>
      <slo expression="Requests&lt;=1000"/>
      <qualifyingCondition expression="ONLYIF AVGRESPONSE&lt;=500"/>
      <compensations xsi:type="mmag:CompensationElement" expression="CREDITS = 10" condition="AVGRESPONSE>5000"/>
    </guaranteeTerms>
  </agTerm>
</mmag:AgreementModel>
