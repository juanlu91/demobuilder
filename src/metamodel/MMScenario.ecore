<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="scenarioDiagram" nsURI="es.us.lsi.sma.demobuilder" nsPrefix="es.us.lsi.sma.demobuilder">
  <eClassifiers xsi:type="ecore:EClass" name="Customer">
    <eStructuralFeatures xsi:type="ecore:EReference" name="serviceNeeds" upperBound="-1"
        eType="#//ServiceNeed"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Provider">
    <eStructuralFeatures xsi:type="ecore:EReference" name="serviceOffers" upperBound="-1"
        eType="#//ServiceOffer"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ServiceNeed">
    <eStructuralFeatures xsi:type="ecore:EReference" name="agreement" eType="#//Agreement"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ServiceOffer">
    <eStructuralFeatures xsi:type="ecore:EReference" name="agreement" eType="#//Agreement"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Scenario">
    <eStructuralFeatures xsi:type="ecore:EReference" name="customers" eType="#//Customer"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="providers" eType="#//Provider"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Agreement"/>
</ecore:EPackage>
