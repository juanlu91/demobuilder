<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="scenarioDiagram" nsURI="es.us.lsi.sma.demobuilder.scenario"
    nsPrefix="es.us.lsi.sma.demobuilder.scenario">
  <eClassifiers xsi:type="ecore:EClass" name="Scenario">
    <eAnnotations source="gmf"/>
    <eAnnotations source="gmf.diagram"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="scenarioElement" upperBound="-1"
        eType="#//ScenarioElement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Consumer" eSuperTypes="#//ScenarioElement #//Actor">
    <eAnnotations source="gmf.node">
      <details key="figure" value="svg"/>
      <details key="svg.uri" value="platform:/plugin/demobuilder.scenario/icons/Consumer.svg"/>
      <details key="label" value="name"/>
      <details key="tool.name" value="Consumer"/>
      <details key="tool.description" value="Creates a consumer"/>
      <details key="label.placement" value="external"/>
      <details key="label.icon" value="false"/>
      <details key="size" value="100,100"/>
      <details key="tool.small.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.small.path" value="/icons/full/obj16/Consumer.gif"/>
      <details key="tool.large.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.large.path" value="/icons/full/obj32/Consumer.gif"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Role">
    <eLiterals name="PROVIDER"/>
    <eLiterals name="CONSUMER" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="AssessmentInterval">
    <eLiterals name="YEARLY"/>
    <eLiterals name="MONTHLY" value="1"/>
    <eLiterals name="WEEKLY" value="2"/>
    <eLiterals name="DAILY" value="3"/>
    <eLiterals name="HOURLY" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="CompensationType">
    <eLiterals name="PENALTY"/>
    <eLiterals name="REWARD" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="GuaranteeTerm" eSuperTypes="#//ScenarioElement">
    <eAnnotations source="gmf.node">
      <details key="label.placement" value="none"/>
      <details key="label.icon" value="false"/>
      <details key="figure" value="svg"/>
      <details key="svg.uri" value="platform:/plugin/demobuilder.scenario/icons/GuaranteeTerm.svg"/>
      <details key="tool.name" value="Guarantee Term"/>
      <details key="tool.description" value="Creates a new guarantee term"/>
      <details key="tool.small.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.small.path" value="/icons/full/obj16/GuaranteeTerm.gif"/>
      <details key="tool.large.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.large.path" value="/icons/full/obj32/GuaranteeTerm.gif"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="expression" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="condition" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="compensationInterval" eType="#//AssessmentInterval"
        defaultValueLiteral="MONTHLY"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="compensationType" eType="#//CompensationType"
        defaultValueLiteral="REWARD"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="compensationExpression"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="compensationCondition"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Provider" eSuperTypes="#//ScenarioElement #//Actor">
    <eAnnotations source="gmf.node">
      <details key="figure" value="svg"/>
      <details key="svg.uri" value="platform:/plugin/demobuilder.scenario/icons/Provider.svg"/>
      <details key="label" value="name"/>
      <details key="tool.name" value="Provider"/>
      <details key="tool.description" value="Creates a provider"/>
      <details key="label.placement" value="external"/>
      <details key="label.icon" value="false"/>
      <details key="size" value="100,100"/>
      <details key="tool.small.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.small.path" value="/icons/full/obj16/Provider.gif"/>
      <details key="tool.large.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.large.path" value="/icons/full/obj32/Provider.gif"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ServiceNeed" eSuperTypes="#//ServiceElement #//ScenarioElement">
    <eAnnotations source="gmf.node">
      <details key="figure" value="svg"/>
      <details key="svg.uri" value="platform:/plugin/demobuilder.scenario/icons/serviceNeed.svg"/>
      <details key="label" value="name"/>
      <details key="label.placement" value="external"/>
      <details key="label.icon" value="false"/>
      <details key="tool.name" value="Service Need"/>
      <details key="tool.description" value="Creates a service need"/>
      <details key="size" value="50,50"/>
      <details key="tool.small.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.small.path" value="/icons/full/obj16/ServiceNeed.gif"/>
      <details key="tool.large.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.large.path" value="/icons/full/obj32/ServiceNeed.gif"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="agreementLink" eType="#//AgreementLink"
        eOpposite="#//AgreementLink/source"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ServiceOffer" eSuperTypes="#//ServiceElement #//ScenarioElement">
    <eAnnotations source="gmf.node">
      <details key="figure" value="svg"/>
      <details key="svg.uri" value="platform:/plugin/demobuilder.scenario/icons/serviceOffer.svg"/>
      <details key="label" value="name"/>
      <details key="label.placement" value="external"/>
      <details key="label.icon" value="false"/>
      <details key="tool.name" value="Service Offer"/>
      <details key="tool.description" value="Creates a service offer"/>
      <details key="size" value="50,50"/>
      <details key="tool.small.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.small.path" value="/icons/full/obj16/ServiceOffer.gif"/>
      <details key="tool.large.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.large.path" value="/icons/full/obj32/ServiceOffer.gif"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="serviceConfiguration" eType="#//ServiceConfiguration"
        containment="true">
      <eAnnotations source="gmf.affixed"/>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="agreementLink" eType="#//AgreementLink"
        eOpposite="#//AgreementLink/target"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ServiceConfiguration" eSuperTypes="#//ScenarioElement">
    <eAnnotations source="gmf.node">
      <details key="label.placement" value="none"/>
      <details key="label.icon" value="false"/>
      <details key="figure" value="svg"/>
      <details key="svg.uri" value="platform:/plugin/demobuilder.scenario/icons/serviceConfiguration.svg"/>
      <details key="tool.name" value="Service Configuration"/>
      <details key="tool.description" value="Creates a service configuration section"/>
      <details key="tool.small.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.small.path" value="/icons/full/obj16/ServiceConfiguration.gif"/>
      <details key="tool.large.bundle" value="demobuilder.scenario.edit"/>
      <details key="tool.large.path" value="/icons/full/obj32/ServiceConfiguration.gif"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="endpointReference" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="definitionReference" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="monitorReference" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="serviceName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="guaranteTerms" upperBound="-1"
        eType="#//GuaranteeTerm" containment="true">
      <eAnnotations source="gmf.affixed"/>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AgreementLink" eSuperTypes="#//ScenarioElement">
    <eAnnotations source="gmf.link">
      <details key="source" value="source"/>
      <details key="target" value="target"/>
      <details key="label" value="name"/>
      <details key="width" value="1"/>
      <details key="style" value="dash"/>
      <details key="tool.name" value="Agreement Link"/>
      <details key="tool.description" value="Creates an agreement link"/>
      <details key="color" value="0,0,0"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//ServiceNeed"
        eOpposite="#//ServiceNeed/agreementLink"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//ServiceOffer"
        eOpposite="#//ServiceOffer/agreementLink"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ServiceLink" eSuperTypes="#//ScenarioElement">
    <eAnnotations source="gmf.link">
      <details key="source" value="actor"/>
      <details key="target" value="serviceElement"/>
      <details key="label.placement" value="none"/>
      <details key="width" value="2"/>
      <details key="style" value="solid"/>
      <details key="tool.name" value="Service Link"/>
      <details key="tool.description" value="Creates a service link"/>
      <details key="color" value="0,0,0"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="actor" eType="#//Actor"
        eOpposite="#//Actor/serviceLinks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="serviceElement" eType="#//ServiceElement"
        eOpposite="#//ServiceElement/serviceLinks"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ScenarioElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ServiceElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="serviceLinks" upperBound="-1"
        eType="#//ServiceLink" eOpposite="#//ServiceLink/serviceElement"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Actor">
    <eStructuralFeatures xsi:type="ecore:EReference" name="serviceLinks" upperBound="-1"
        eType="#//ServiceLink" eOpposite="#//ServiceLink/actor"/>
  </eClassifiers>
</ecore:EPackage>
